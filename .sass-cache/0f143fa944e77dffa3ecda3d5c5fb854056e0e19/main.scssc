@import "compass/reset";

@import "variables";



/* ------------------------------------------------------ */

//To use rems like pixel sizes:
@function calculateRem($size) {
  $remSize: $size / 16px;
  @return #{$remSize}rem;
}

//use this for setting font-size
@mixin remSize($size) {
  font-size: $size; //Fallback in px
  font-size: calculateRem($size);
}


@function calculateImageHeight($url) {
	$height: image-height($url);
	@return #{$height}px;
}


/* ==========================================================================
        BASICS
   ========================================================================== */


html {
	font-size: 16px;
	line-height: 24px;

	font-family: "robot-thin", san-serif;
}


body {
	font-family: "robot", sans-serif;
	color: $white;
	font-weight: 200;
	background-color: $dark;
}

h1 {
	font-size: calculateRem(120px);
	text-align: center;
	margin-bottom: calculateRem(40px);
	line-height: calculateRem(140px);
	font-family: "chalk", sans-serif;
}


h2 {
	font-size: calculateRem(36px);
	margin-bottom: calculateRem(15px);
	font-family: "robot-boldIt";
	line-height: calculateRem(46px);
}

h3 {
	text-align: center;
	font-size: calculateRem(28px);
	line-height: calculateRem(32px);
	margin-bottom: calculateRem(20px);
	font-family: "robot-thin"
}

a {
	text-decoration: none;
	color: $white;

	&:hover {
		color: $dark;
	}
}

p {
	line-height: calculateRem(30px);
	font-size: calculateRem(20px);
	font-family: "robot-thin";
}

.width-sizer {
	max-width: 1200px;
	margin: auto;
	display: block;
}

section {
	width: 100%;
	overflow: hidden;
}


.fullHeightSection {
	height: 80vh;
}

.section-Padding {
	padding: calculateRem(70px) $main-sidePadding;
	overflow: hidden;
}


.main-btn {
	display: inline-block;
	background-color: $pink;
	padding: calculateRem(10px);
	font-family: "robot-thin", san-serif;
	font-size: calculateRem(20px);
	font-style: italic;
}

.backgroundImage {
	display: none;
}


.column-left {
	display: inline-block;
	width: calc(50% - 5px);
	margin-right: 10px; 
	overflow: hidden;
}

.column-right {
	display: inline-block;
	width: calc(50% - 10px);
	overflow: hidden;

}


@media only screen and (max-width: $tablet-width) {

	html {
		font-size: 12px;
	} 

	
	
}
@media only screen and (max-width: $mobile-width) {

	p {
		font-size: calculateRem(18px);
		line-height: calculateRem(24px);
	}
}
/* ==========================================================================
       HEADER
   ========================================================================== */
header {
	position: fixed;
	top: 0;
	z-index: 100;
	//height: 45px;
	background-color: $darkBlue;
	width: 100%;

	.module-padding {
		padding: 10px $main-sidePadding;
		overflow: hidden;
		text-align: right;
	}

	.filler {
		height: 45px;
		width: 1px;
		display: inline-block;
	}
}


#header-logo {
	float: left;

	img {
		height: 45px;
	}
}

#header-nav {
	overflow: hidden;
	display: inline-block;

	li {
		display: inline-block;
		text-decoration: none;
		margin-left: 5px;

		a {
			text-transform: uppercase;

			&:hover {
				color: $pink;
			}
		}
	}
}

#burger-button {
	display: none;
}


@media only screen and (max-width: $mobile-width) {
	#burger-button {
		display: inline-block;
		img {
			height: 35px;
		}
	}

	#header-nav {
		display: none;
	
		display: block;
		text-align: center;
		width: 100%;

		li {
			display: block;
			margin-bottom: 0px;
			padding: 10px;

			a {
				display: block;
				padding: 10px 0;
			}
		}
	}
	
	header {
		
		font-size: 24px;

		.module-padding {
			padding: 10px 30px;
		}

		.filler {
			display: none;
		}
	}

	#header-logo img{
		height: 35px;
	}

	
}

/* ==========================================================================
      FOOTER
   ========================================================================== */
footer#footer {
	font-size: calculateRem(18px);
	background-color: $dark;
	color: $white;

	p {
		margin-bottom: 10px;
	}

	h3 {
		text-align: left;
	}

	img {
		width: 20%;
		margin-right: 3%;

		vertical-align: top;

		&:nth-of-type {
			margin-right: 0;
			float: right;
		}
	}

	.footer-logos {
		display: inline-block;
		img {
			height: 100px;
			margin-right: 10px;
		}
	}

	#footer-nav {
		width: 100%;
		// margin-top: calculateRem(20px);

		li {
			display: inline-block;
			margin-right:10px;

			text-transform: uppercase;

		}
	}
	
	a {
		color: $white;

		&:hover {
			color: $lightBlue;
		}
	}
}



